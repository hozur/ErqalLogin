<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.almj.erqallogin" >

    <application
        android:name="com.almj.erqallogin.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.almj.erqallogin.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.almj.erqallogin.LoginActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.almj.erqallogin.SignupActivity" android:theme="@style/AppTheme.Dark" />
    </application>

</manifest>